{
  "name": "sample-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8",
    "react-router-dom": "^4.3.1",
    "node-sass": "^4.9.4",
    "node-sass-chokidar": "^1.3.4",
    "bootstrap": "^4.1.3",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "start": "npm run build:css; (npm run watch:css & PORT=3000 react-scripts start)",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "node-sass-chokidar src/styles/main.scss -o src/styles/",
    "watch:css": "node-sass-chokidar src/styles -o src/styles --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
