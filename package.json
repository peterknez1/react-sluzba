{
  "name": "sample-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.0.0",
    "@ckeditor/ckeditor5-editor-classic": "^12.0.0",
    "@ckeditor/ckeditor5-react": "^1.1.3",
    "bootstrap": "^4.1.3",
    "node-sass": "^4.9.4",
    "node-sass-chokidar": "^1.3.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "start-react": "react-scripts start ",
    "start": "npm-run-all -p watch:css start-react",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:css": "node-sass-chokidar src/styles/style.scss -o src/styles/",
    "watch:css": "node-sass-chokidar src/styles -o src/styles --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@ckeditor/ckeditor5-alignment": "^11.0.0",
    "npm-run-all": "^4.1.5"
  }
}
